#include <a_samp>
#include <cef>

#define INTERFACE_BROWSER_ID 0xABCDE
// change url to yours
#define INTERFACE_BROWSER_URL "http://localhost:5000/cef2.html"

forward OnPwdClose(player_id, const arguments[]);
forward OnPwdTry(player_id, const arguments[]);

public OnPwdClose(player_id, const arguments[]) {
	// just kick  a player when he closes the window
	Kick(player_id);
    return 1;
}

public OnPwdTry(player_id, const arguments[]) {
    new result = strcmp(arguments, "mycoolpwd") != 0;
	
	cef_emit_event(player_id, "pwd:response", CEFINT(result));
	
    return 1;
}

public OnCefInitialize(player_id, success) {
    if (success == 1) {
        cef_create_browser(player_id, INTERFACE_BROWSER_ID, INTERFACE_BROWSER_URL, false, true);
        return;
    }

    SendClientMessage(player_id, -1, "Ahh to bad you cannot see our new cool interface ...");
}

